@model OMM.App.Models.ViewModels.AssignmentsListViewModel

<h4 class="mb-3 text-center">Assignments From Me</h4>
<hr class="hr-oom" style="width:300px;" />
<h5 class="mb-3 text-center"><button type="button" class="btn btn-success btn-sm" disabled>On going</button></h5>

<partial name="_FromMeAssignmentsOngoingPartial" model="@Model.OngoingAssignments" />

<hr class="hr-oom mt-3" style="width:300px;" />
<h5 class="mb-3 text-center">
    <button type="button" class="btn btn-secondary btn-sm" data-toggle="collapse" data-target="#collapseCompletedAssignments" aria-expanded="false" aria-controls="collapseCompletedAssignments">
        Completed
    </button>
</h5>
<hr class="hr-oom" style="width:300px;" />

<partial name="_FromMeAssignmentsCompletedPartial" model="@Model.CompletedAssignments" />

@section Scripts{
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/js/run-dataTable.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.6/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.bootstrap4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.print.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.colVis.min.js"></script>

}